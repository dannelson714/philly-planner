t date\n  // fill out budgert and ethiccnic food\n  // needs banner at top with login logout info\n  return (\n    <motion.div\n      id=\"dinner\"\n      className=\"card card-rounded w-50\"\n      initial={{ y: -250 }}\n      animate={{ y: 0 }}\n      transition={{ type: \"spring\", stiffness: 100 }}\n    >\n      <div className=\"card-header bg-dark text-center\">\n        <h1>Plan The Perfect Philly Night!</h1>\n      </div>\n      <form>\n        <div className=\"card-body m-5\">\n          <label>Select Budget Range</label>\n          <select\n            name=\"budget\"\n            value={formState.budget}\n            onChange={handleChange}\n          >\n            {\" \"}\n            <option value=\"1\">$</option>\n            <option value=\"2\">$$</option>\n            <option value=\"3\">$$$</option>\n            <option value=\"4\">$$$$</option>\n          </select>\n        </div>\n      </form>\n      <div className=\"dinnerCard card-body m-5\">\n        <label className=\"dinnerText\">Whatcha Feelin' For Dinner?</label>\n        <br></br>\n        <input\n          onChange={handleChange}\n          name=\"dinnerOption\"\n          value={formState.dinnerOption}\n          placeholder=\"Italian, Mexican, etc..\"\n        ></input>\n      </div>\n\n      <div className=\"card-footer text-center m-3\">\n        <h2>Ready to Move To The Next Step?</h2>\n        {/* <Link to=\"/matchup\"> */}\n        <button onClick={handleFormSubmit} className=\"btn btn-lg btn-danger\">\n          Plan Your Next Step\n        </button>\n        {/* </Link> */}\n      </div>\n    </motion.div>\n  );\n};\n\nexport default Dinner;\n","/Users/danielnelson/Desktop/UPenn/projects/project3/NFT-Search/client/src/pages/Components/Login.js",[],"/Users/danielnelson/Desktop/UPenn/projects/project3/NFT-Search/client/src/pages/Components/display.js",["74","75","76","77","78","79"],"import { useEffect, useState } from \"react\";\nimport { AnimatePresence, motion } from \"framer-m